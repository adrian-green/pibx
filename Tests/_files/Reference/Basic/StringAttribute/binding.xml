<?xml version="1.0"?>
<binding>
  <namespace uri="http://www.w3.org/2002/ws/databinding/examples/6/09/" default="elements"/>
  <mapping class="StringAttribute" name="stringAttribute">
    <structure map-as="StringAttribute"/>
  </mapping>
  <mapping class="StringAttribute" abstract="true" type-name="StringAttribute">
    <value style="element" name="text" get-method="getText" set-method="setText" usage="optional"/>
    <value style="attribute" name="string" get-method="getString" set-method="setString" usage="optional"/>
  </mapping>
  <mapping class="EchoStringAttribute" name="echoStringAttribute">
    <structure map-as="StringAttribute" get-method="getStringAttribute" set-method="setStringAttribute" name="stringAttribute"/>
  </mapping>
</binding>
