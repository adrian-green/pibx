<?xml version="1.0"?>
<binding>
  <namespace uri="http://www.w3.org/2002/ws/databinding/examples/6/09/" default="elements"/>
  <mapping class="BooleanElement" name="booleanElement">
    <value style="text" get-method="isBooleanElement" set-method="setBooleanElement"/>
  </mapping>
  <mapping class="EchoBooleanElement" name="echoBooleanElement">
    <structure type="BooleanElement" get-method="getBooleanElement" set-method="setBooleanElement"/>
  </mapping>
</binding>
