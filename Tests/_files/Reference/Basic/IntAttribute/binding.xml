<?xml version="1.0"?>
<binding>
  <namespace uri="http://www.w3.org/2002/ws/databinding/examples/6/09/" default="elements"/>
  <mapping class="IntAttribute" name="intAttribute">
    <structure map-as="IntAttribute"/>
  </mapping>
  <mapping class="IntAttribute" abstract="true" type-name="IntAttribute">
    <value style="element" name="text" get-method="getText" set-method="setText" usage="optional"/>
    <value style="attribute" name="int" get-method="getInt" set-method="setInt" usage="optional"/>
  </mapping>
  <mapping class="EchoIntAttribute" name="echoIntAttribute">
    <structure map-as="IntAttribute" get-method="getIntAttribute" set-method="setIntAttribute" name="intAttribute"/>
  </mapping>
</binding>
