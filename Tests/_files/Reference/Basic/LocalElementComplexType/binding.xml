<?xml version="1.0"?>
<binding>
  <namespace uri="http://www.w3.org/2002/ws/databinding/examples/6/09/" default="elements"/>
  <mapping class="LocalElementComplexType" name="localElementComplexType">
    <structure map-as="LocalElementComplexType"/>
  </mapping>
  <mapping class="LocalElementComplexType" abstract="true" type-name="LocalElementComplexType">
    <structure name="name">
      <value style="element" name="firstName" get-method="getNameFirstName" set-method="setNameFirstName"/>
      <value style="element" name="lastName" get-method="getNameLastName" set-method="setNameLastName"/>
    </structure>
  </mapping>
  <mapping class="EchoLocalElementComplexType" name="echoLocalElementComplexType">
    <structure map-as="LocalElementComplexType" get-method="getLocalElementComplexType" set-method="setLocalElementComplexType" name="localElementComplexType"/>
  </mapping>
</binding>
