<?xml version="1.0"?>
<binding>
  <namespace uri="http://www.w3.org/2002/ws/databinding/examples/6/09/" default="elements"/>
  <mapping class="BooleanAttribute" name="booleanAttribute">
    <structure map-as="BooleanAttribute"/>
  </mapping>
  <mapping class="BooleanAttribute" abstract="true" type-name="BooleanAttribute">
    <value style="element" name="text" get-method="getText" set-method="setText" usage="optional"/>
    <value style="attribute" name="boolean" get-method="getBoolean" set-method="setBoolean" usage="optional"/>
  </mapping>
  <mapping class="EchoBooleanAttribute" name="echoBooleanAttribute">
    <structure map-as="BooleanAttribute" get-method="getBooleanAttribute" set-method="setBooleanAttribute" name="booleanAttribute"/>
  </mapping>
</binding>
